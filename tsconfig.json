// prettier-ignore
{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "types": ["react", "react-native", "jest", "styled-components"],
    "baseUrl": ".",
    "paths": {
      "*": ["src/*"],
      "@assets/*": ["src/assets/*"],
      "@components/*": ["src/components/*"],
      "@hooks/*": ["src/hooks/*"],
      "@interfaces/*":["src/interfaces/*"],
      "@pages/*": ["src/pages/*"],
      "@routes/*": ["src/routes/*"],
      "@services/*": ["src/services/*"],
      "@styles/*": ["src/styles/*"],
      "@utils/*":["src/utils/*"],
    },
    "noImplicitAny": false,
    "suppressExcessPropertyErrors": true,
    "skipLibCheck": true
  }
}
